{
  "compilerOptions": {
    "target": "es5",
  "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["*"]
    },
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "ignoreDeprecations": "6.0"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
